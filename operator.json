{
  "name": "Seurat",
  "description": "Imports a Seurat object saved as an RData or RDS file.",
  "tags": [
    "import",
    "sc-RNA-seq"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/read_seurat_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Document ID",
            "type": "string",
            "description": "Document ID of Seurat RData or RDS file.",
            "ontologyMapping": "document",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "description": "Sparse matrix output with gene, cell metadata and Seurat annotations",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Document ID"
              ],
              "rColumns": [
                "Document ID"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "description": "Includes Seurat metadata columns dynamically",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "gene_id",
                  "type": "int32"
                },
                {
                  "kind": "Attribute",
                  "name": "cell_id",
                  "type": "int32"
                },
                {
                  "kind": "Attribute",
                  "name": "value",
                  "type": "double"
                },
                {
                  "kind": "Attribute",
                  "name": "gene_names",
                  "type": "string"
                },
                {
                  "kind": "Attribute",
                  "name": "cell_names",
                  "type": "string"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}